set group_subprocesses Auto
set ignore_six_quark_processes False
set loop_optimized_output True
set complex_mass_scheme False
import model sm
define p = 21 2 4 1 3 -2 -4 -1 -3 5 -5
define j = p
define l+ = e+ mu+
define l- = e- mu-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~
import model VLQ_v4_4FNS_UFO
define bb = b b~
define WW = w+ w-
define tt = t t~
define ferm =  ve vm vt ve~ vm~ vt~ u c d s b u~ c~ d~ s~ b~ e- mu- ta- e+ mu+ ta+
define Topo = tp tp~
generate p p > j bb Topo  / h , ( Topo > h tt / z w+ w-, tt > ferm ferm bb, h > b b~)
output SingleTth_VariableMassH_MT700_MH450 --nojpeg
